{
    "type": "project",
    "license": "proprietary",
    "require": {
        "php": "^7.2.5",
        "ext-ctype": "*",
        "ext-iconv": "*",
        "beberlei/doctrineextensions": "^1.2",
        "cocur/slugify": "^4.0",
        "doctrine/doctrine-migrations-bundle": "^3.0",
        "egulias/email-validator": "^2.1",
        "friendsofsymfony/ckeditor-bundle": "^2.2",
        "friendsofsymfony/jsrouting-bundle": "^2.6",
        "helios-ag/fm-elfinder-bundle": "^10.0",
        "sensio/framework-extra-bundle": "^5.1",
        "stripe/stripe-php": "^7.40",
        "symfony/apache-pack": "^1.0",
        "symfony/asset": "5.1.3",
        "symfony/console": "5.1.3",
        "symfony/dotenv": "5.1.3",
        "symfony/expression-language": "5.1.3",
        "symfony/flex": "^1.3.1",
        "symfony/form": "5.1.3",
        "symfony/framework-bundle": "5.1.3",
        "symfony/http-client": "5.1.3",
        "symfony/intl": "5.1.3",
        "symfony/mailer": "5.1.3",
        "symfony/mime": "5.1.3",
        "symfony/monolog-bundle": "^3.1",
        "symfony/notifier": "5.1.3",
        "symfony/orm-pack": "*",
        "symfony/process": "5.1.3",
        "symfony/security-bundle": "5.1.3",
        "symfony/serializer-pack": "*",
        "symfony/string": "5.1.3",
        "symfony/translation": "5.1.3",
        "symfony/twig-bundle": "5.1.3",
        "symfony/validator": "5.1.3",
        "symfony/web-link": "5.1.3",
        "symfony/webpack-encore-bundle": "^1.7",
        "symfony/yaml": "5.1.3",
        "twig/cssinliner-extra": "^3.0",
        "twig/extensions": "^1.5",
        "twig/extra-bundle": "^3.0",
        "twig/intl-extra": "^3.0",
        "twig/string-extra": "^3.0",
        "twig/twig": "^2.12",
        "vich/uploader-bundle": "^1.14"
    },
    "require-dev": {
        "symfony/debug-pack": "*",
        "symfony/maker-bundle": "^1.0",
        "symfony/profiler-pack": "*",
        "symfony/test-pack": "*"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Tests\\": "tests/"
        }
    },
    "replace": {
        "paragonie/random_compat": "2.*",
        "symfony/polyfill-ctype": "*",
        "symfony/polyfill-iconv": "*",
        "symfony/polyfill-php72": "*",
        "symfony/polyfill-php71": "*",
        "symfony/polyfill-php70": "*",
        "symfony/polyfill-php56": "*"
    },
    "scripts": {
        "auto-scripts": {
            "cache:clear": "symfony-cmd",
            "assets:install %PUBLIC_DIR%": "symfony-cmd",
            "elfinder:install": "symfony-cmd"
        },
        "post-install-cmd": [
            "@auto-scripts"
        ],
        "post-update-cmd": [
            "@auto-scripts"
        ]
    },
    "conflict": {
        "symfony/symfony": "*"
    },
    "extra": {
        "symfony": {
            "allow-contrib": false,
            "require": "5.1.3"
        }
    }
}
